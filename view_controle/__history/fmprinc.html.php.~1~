        <script type="text/javascript">
            $(document).ready(function () {
                $("a.ActiveButton").bind({mousedown: function () {
                        if ($(this).attr('disabled') === undefined)
                            $(this).addClass('Activated');
                    }, mouseleave: function () {
                        if ($(this).attr('disabled') === undefined)
                            $(this).removeClass('Activated');
                    }, mouseup: function () {
                        if ($(this).attr('disabled') === undefined)
                            $(this).removeClass('Activated');
                    }});
            });
        </script>
